{
  "name": "forkgen",
  "version": "0.0.0",
  "description": "A web (localhost) based open source tool for GUI for Git that allows you control Git without using the command line.",
  "scripts": {
    "start": "node ./server/server.js",
    "dev": "webpack -d --watch",
    "build": "webpack -p",
    "test": "jest --no-cache",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hegdeashwin/forkgen"
  },
  "keywords": [
    "Git",
    "Github",
    "BitBucket",
    "versioning",
    "distributed"
  ],
  "author": "Ashwin Hegde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hegdeashwin/forkgen/issues"
  },
  "homepage": "https://github.com/hegdeashwin/forkgen",
  "dependencies": {
    "axios": "^0.9.1",
    "body-parser": "~1.15.0",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.3",
    "cookie-parser": "~1.4.1",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "file-loader": "^0.8.5",
    "flux": "^2.1.1",
    "font-awesome": "^4.5.0",
    "highcharts": "^4.2.3",
    "history": "^2.0.1",
    "html-webpack-plugin": "^2.16.1",
    "joi": "^8.0.5",
    "morgan": "~1.7.0",
    "node-dir": "^0.1.10",
    "nodegit": "^0.12.2",
    "nodemailer": "^1.4.0",
    "react": "^15.1.0",
    "react-data-components": "^0.6.0",
    "react-dom": "^15.1.0",
    "react-grid-layout": "^0.12.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "react-widgets": "^3.3.1",
    "redux": "^3.4.0",
    "shelljs": "^0.6.0",
    "react-bootstrap": "^0.29.4"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-jest": "^11.0.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "jest": "^12.1.1",
    "enzyme": "^2.3.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "react-addons-test-utils": "^15.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  },
  "jest": {
    "moduleFileExtensions": ["js", "jsx"],
    "unmockedModulePathPatterns": [
      "./node_modules/react/",
      "./node_modules/react-dom/",
      "./node_modules/react-addons-test-utils/",
      "./node_modules/enzyme/"
    ]
  }
}
